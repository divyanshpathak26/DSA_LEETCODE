class Solution {
public:
    int climbStairs(int n) {
        int a=1,b=1 ,c;
      if(n ==1)
      return 1;
      else if(n==0)
      return 0;
      else{
    


        for(int i=1;i<n;i++){

            c=a+b;
            b=a;
            a=c;
        }
      }
      return a;


        
    }
};
